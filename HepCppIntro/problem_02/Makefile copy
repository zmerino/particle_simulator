# W. H. Bell
# A Makefile to build FileIO.exe

CC=g++
TARGET=encoder
OBJECTS=solution_02.cpp

$(TARGET).exe: $(OBJECTS)
	@echo "**"
	@echo "** Linking Executable"
	@echo "**"
	$(CC) $(OBJECTS) -o $(TARGET)
	@echo "**"
	@echo "** execute ./encoder private_message.txt -e"
	./encoder private_message.txt -e

